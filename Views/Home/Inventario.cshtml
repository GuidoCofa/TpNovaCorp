@{
    ViewData["Title"] = "Inventario";
}
<Button type="button" class = "btn btn-primary" data-toggle = "modal" data-target = "#ModalObjeto" onclick = "MostrarObjeto(@ViewBag.id_objeto)">
Ver Info En modal
</Button>


<div class="modal fade" id="ModalObjeto" tabindex="-1" aria_labelledby="exampleModalObjeto" aria_hidden="true">
<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title"  id="NombreObjeto">Modal Title</h5>
    <button type = "button" class="close" data-dismiss="modal" arial-label="Close">
    </button>
</div>
<div class="modal-body">
@foreach (Objeto item in ViewBag.Objeto)
{
            <tr>
                <td>@item.id_objeto</td>
                <td>@item.tipo_objeto</td>
                <td>@item.Nombre_Obj</td>
                <td>@item.stock</td>
                <td>@item.lugar</td>
            </tr>
}  
           